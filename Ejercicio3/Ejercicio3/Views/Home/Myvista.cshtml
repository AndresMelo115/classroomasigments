@{    
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>

        @{
            for (int i = 1; i <= 12; i++)
            {
                <div> <br></div>
                <div>Tabla del @i<br></div>
                <div> <br></div>

                for (int j = 1; j <= 12; j++)
                {
                    <div>@i x @j = @(i * j)<br></div>
                }
            }
        }
    </div>
</body>
</html>
